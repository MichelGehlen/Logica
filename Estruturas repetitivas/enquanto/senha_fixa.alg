algoritmo "senha_fixa"

var
   senha : inteiro
inicio


   escreval("Digite a senha: ")
   leia(senha)
   
   enquanto senha <> 2312 faca
   
   escreval("Senha Invalida tente novamente: " )
   leia(senha)
   
   fimenquanto
   
   se (senha = 2312) entao
   escreval("Acesso permitido")
   fimse
   

fimalgoritmo
