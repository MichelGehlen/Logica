algoritmo "crescente"

var
  x , y : inteiro
inicio
      escreval("Digite dois numeros: ")

    enquanto x <> y faca
    
    escreval("Digite dois numeros: ")
    leia(x,y)

    se x > y entao
    escreval("DECRESCENTE")
    senao
    escreval("CRESCENTE")
    fimse
    fimenquanto

     

fimalgoritmo
