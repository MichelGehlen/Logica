algoritmo "teste_saida"

var
      a : inteiro
      b : real
      c : caractere
      d : logico
inicio
      a <- 32
      b <- 2.3213
      c <- "bom dia"
      d <- verdadeiro
      
      escreval(a)
      escreval(b)
      escreval(b:10)//imprime o valor com os espaços colocados no codigo
      escreval(c)
      escreval(d)
fimalgoritmo
